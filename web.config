<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <handlers>
            <add name="Python Interpreter" path="*.py" verb="*" modules="CgiModule" scriptProcessor="D:\Deployment\venv\Scripts\python.exe %s %s" resourceType="File" />
            <add name="FirstFlaskHandler" path="*" verb="*" modules="FastCgiModule" scriptProcessor="D:\Deployment\venv\Scripts\python.exe|D:\Deployment\venv\Lib\site-packages\wfastcgi.py" resourceType="Unspecified" />
        </handlers>
    </system.webServer>
</configuration>
